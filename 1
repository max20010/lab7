
#include "pch.h"
#include <iostream>
using namespace std;
int main()
{
	setlocale(LC_ALL, "ru");
	int a, b;
	cout << "Введите A: ";
	cin >> a;
	cout << "Введите B: ";
	cin >> b;
	if (a > 2 && b <= 3) {
		cout << "Неравенства А > 2 и В <= 3 верны" << endl;
	}
	else
	{
		cout << "Неравенства А > 2 и В <= 3 неверны" << endl;
	}
}
