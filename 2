#include "pch.h"
#include <iostream>
using namespace std;
int main()
{
	setlocale(LC_ALL, "ru");
	int a;
	cout << "Введите A: ";
	cin >> a;

	if (a>=10 && a<=99 && a%2==0) {
		cout << "Данное число является чётным двузначным" << endl;
	}
	else
	{
		cout << "Данное число не является чётным двузначным" << endl;
	}
}
