#include "pch.h"
#include <iostream>
using namespace std;
int main()
{
	setlocale(LC_ALL, "ru");
	int a,b,c;
	cout << "Введите A: ";
	cin >> a;	
	cout << "Введите B: ";
	cin >> b;	
	cout << "Введите C: ";
	cin >> c;
	if (a<b&&b<c) {
		cout << "Справедливо двойное неравенство A<B<C" << endl;
	}
	else
	{
		cout << "Двойное неравенство A<B<C не справедливо" << endl;
	}
}
